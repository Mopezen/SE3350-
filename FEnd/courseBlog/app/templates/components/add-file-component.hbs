
<div class="row">
	{{#file-picker fileLoaded="fileLoaded" accept=".xlsx" readAs="readAsBinaryString"}}
		Drag here or click to upload a file
	{{/file-picker}}

	<form action='studentInputFiles' method='POST' enctype='multipart/form-data'>
	    <input type='file' name='userFile'><br>
	    <input type='submit' name='upload_btn' value='upload'>
	</form>

	{{add-file-component url="/upload"}}
</div>
